<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee">
  <display-name>TrackMe</display-name>
  <welcome-file-list>
    <welcome-file>html/Login.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>TrackMeServlet</display-name>
    <servlet-name>TrackMeServlet</servlet-name>
    <servlet-class>com.trackMe.web.TrackMeServlet</servlet-class>
  </servlet>
  <servlet>
     <description></description>
    <display-name>Live</display-name>
    <servlet-name>Live</servlet-name>
    <servlet-class>com.trackMe.web.Live</servlet-class>
  </servlet>
  <listener>
    <listener-class>com.trackMe.web.AppServletContextListener</listener-class>
  </listener>
  <servlet-mapping>
    <servlet-name>TrackMeServlet</servlet-name>
    <url-pattern>/TrackMeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Live</servlet-name>
    <url-pattern>/Live</url-pattern>
  </servlet-mapping>
</web-app>